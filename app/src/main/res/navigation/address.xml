<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/address"
    app:startDestination="@id/addressFragment">

    <fragment
        android:id="@+id/addressFragment"
        android:name="com.alobosz.bitcoinbeetrack.presentation.addresses.AddressFragment"
        android:label="Address"
        tools:layout="@layout/fragment_address_creator" >
        <action
            android:id="@+id/action_addressFragment_to_successFragment"
            app:destination="@id/successFragment" />
    </fragment>
    <fragment
        android:id="@+id/successFragment"
        android:name="com.alobosz.bitcoinbeetrack.presentation.addresses.SuccessFragment"
        android:label="SuccessFragment" />
</navigation>